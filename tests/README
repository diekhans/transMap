    hgGetAnn hg19 all_mrna chr7:113480010-114709562 stdout | cut -f 10 | sort -u | head -15 >input/rna.acc 
    hgGetAnn hg19 all_est chr7:113480010-114709562 stdout | cut -f 10 | sort -u | head -20 >input/est.acc 

