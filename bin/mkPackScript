#!/bin/sh -e
usage='mkPackScript version db1 ..
generate transMapPack script
'
set -beEu -o pipefail

if [ $# -lt 2 ] ; then
    echo "wrong # args: $usage"
    exit 1
fi
version="$1" ; shift
dbs="$*"

for db in $dbs ; do
     hgsql -Ne 'show tables like "transMap%'${version}'"' $db | awk -v db=$db '{print "doPack",db,$1}'
done
